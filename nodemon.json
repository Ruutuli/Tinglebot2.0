{
  "watch": [
    "embeds/",
    "index.js",
    "commands/",
    "handlers/",
    "database/",
    "models/",
    "modules/",
    "utils/",
    "dashboard/",
    "scripts/",
    "scheduler.js"
  ],
  "ignore": ["node_modules/"],
  "ext": "js,json",
  "exec": "set NODE_ENV=development&& node index.js",
  "legacy-watch": true,
  "exitcrash": true,
  "restartable": "rs",
  "delay": "1000",
  "maxRestarts": 10,
  "restartDelay": 3000
}