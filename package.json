{
 "name": "tinglebot-2.0",
 "version": "1.0.0",
 "description": "For ocs stuff",
 "main": "index.js",
 "engines": {
  "node": ">=18.0.0"
 },
 "scripts": {
  "start": "cross-env NODE_ENV=development node --max-old-space-size=512 --optimize-for-size index.js",
  "dev": "cross-env NODE_ENV=development PORT=5000 nodemon index.js",
  "prod": "cross-env NODE_ENV=production PORT=5001 nodemon index.js",
  "both": "concurrently \"npm run dev\" \"npm run prod\"",
  "scheduler": "cross-env NODE_ENV=production node scripts/scheduler.js",
  "deploy-commands": "cross-env NODE_ENV=development node scripts/deploy-commands.js",
  "deploy": "cross-env NODE_ENV=production node scripts/deploy-commands.js"
 },
 "author": "Ruu",
 "license": "ISC",
 "dependencies": {
  "@discordjs/builders": "^1.10.1",
  "@google-cloud/storage": "^7.16.0",
  "axios": "^1.8.4",
  "bottleneck": "^2.19.5",
  "discord.js": "^14.18.0",
  "dotenv": "^16.5.0",
  "figlet": "^1.8.1",
  "google-auth-library": "^9.15.1",
  "googleapis": "^148.0.0",
  "jimp": "^0.22.10",
  "jpeg-js": "^0.4.4",
  "mathjs": "^14.4.0",
  "mersenne-twister": "^1.1.0",
  "moment": "^2.30.1",
  "mongodb": "^6.16.0",
  "mongoose": "^8.13.2",
  "multer": "^1.4.5-lts.1",
  "node-cache": "^5.1.2",
  "node-cron": "^3.0.3",
  "node-fetch": "^3.3.2",
  "sharp": "^0.34.1",
  "tough-cookie": "^4.1.3",
  "uuid": "^11.1.0"
 },
 "devDependencies": {
  "@eslint/js": "^9.25.1",
  "concurrently": "^9.1.2",
  "cross-env": "^7.0.3",
  "dependency-cruiser": "^16.10.1",
  "eslint": "^9.25.1",
  "eslint-plugin-import": "^2.31.0",
  "globals": "^16.0.0"
 }
}
